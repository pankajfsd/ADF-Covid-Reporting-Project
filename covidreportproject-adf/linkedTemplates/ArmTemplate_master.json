{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covidreportproject-adf"
		},
		"ls_ablob_covidprojsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidprojsa'"
		},
		"ls_adls_covidprojadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidprojadls'"
		},
		"ls_databricks_covid_cluster_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks_covid_cluster'"
		},
		"ls_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_db'"
		},
		"ls_adls_covidprojadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidprojadls.dfs.core.windows.net/"
		},
		"ls_http_ecdc_raw_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"ls_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "covid-sqldb-server.database.windows.net"
		},
		"ls_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "covid-db"
		},
		"ls_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sql-admin"
		},
		"tr_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/c9939a34-52b0-4a0a-9cb5-6a03f1daff58/resourceGroups/covid-proj-rg/providers/Microsoft.Storage/storageAccounts/covidprojsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covidreportproject-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidprojsa_connectionString": {
						"value": "[parameters('ls_ablob_covidprojsa_connectionString')]"
					},
					"ls_adls_covidprojadls_accountKey": {
						"value": "[parameters('ls_adls_covidprojadls_accountKey')]"
					},
					"ls_databricks_covid_cluster_accessToken": {
						"value": "[parameters('ls_databricks_covid_cluster_accessToken')]"
					},
					"ls_sql_db_password": {
						"value": "[parameters('ls_sql_db_password')]"
					},
					"ls_adls_covidprojadls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidprojadls_properties_typeProperties_url')]"
					},
					"ls_http_ecdc_raw_data_properties_typeProperties_url": {
						"value": "[parameters('ls_http_ecdc_raw_data_properties_typeProperties_url')]"
					},
					"ls_sql_db_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_server')]"
					},
					"ls_sql_db_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_database')]"
					},
					"ls_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covidreportproject-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covidreportproject-adf_ArmTemplate_0')]"
			]
		}
	]
}